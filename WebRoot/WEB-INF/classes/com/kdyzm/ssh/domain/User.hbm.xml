<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.kdyzm.ssh.domain.User" catalog="test" table="user">
		<id name="uid" type="long" column="uid" length="20">
			<generator class="increment"></generator>
		</id>
		<property name="name" column="name" length="32" type="string"></property>
		<property name="age" column="age" length="11" type="integer"></property>
	</class>
</hibernate-mapping>
